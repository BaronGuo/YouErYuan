<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wzt.sun.infanteducation"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/> 
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET" /> 
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.wzt.sun.infanteducation.BaseApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.BaseActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.LoginActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.FoodActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.ConsultActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.CourseActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.DynamicActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.EvaluateActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.FeedbackActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.IntroductionActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.PlanActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.StarActivity"></activity>
        <activity android:name="com.wzt.sun.infanteducation.activity.DynamicInfoActivity"></activity>
    </application>

</manifest>
